{
  "ignorePaths": [
    "**/.git/**",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.history/**",
    "**/.pixi/**",
    "**/megalinter-reports/**",
    "branding/*.svg",
    "megalinter",
    "megalinter-report/**",
    "report",
    ".vscode",
    ".pnpm-lock.yaml",
    "package-lock.json"
  ],
  "ignoreRegExpList": [
    "/(?<=---[\\n\\r]+(?:.*[\\n\\r])*)\\s*id: \\w+(?=[\\n\\r]+.|[\\n\\r]+---)+/gm"
  ],
  "language": "en",
  "noConfigSearch": true,
  "version": "0.2",
  "words": [
    "Ablist",
    "ACCESSTOKEN",
    "cryosphere",
    "DXXXX",
    "Flesch",
    "Frontmatter",
    "grype",
    "Joblint",
    "Kincaid",
    "lefthook",
    "markdownlint",
    "Navi",
    "Nygard",
    "pixi",
    "runme",
    "secretlintrc",
    "UNACCRUED",
    "unenforceability",
    "venv"
  ]
}
