{
  "ignorePaths": [
    "**/.git/**",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.history/**",
    "**/.pixi/**",
    "**/megalinter-reports/**",
    "branding/*.svg",
    "megalinter",
    "megalinter-report/**",
    "report",
    ".vscode",
    ".pnpm-lock.yaml",
    "package-lock.json"
